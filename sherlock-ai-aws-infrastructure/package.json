{
  "name": "sherlock-ai-legal-database",
  "version": "1.0.0",
  "description": "AWS CDK infrastructure for Sherlock AI Legal Case Management System",
  "main": "lib/sherlock-ai-stack.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
