    "ts-node": "^10.9.1",
    "typescript": "~4.9.5"
  },
  "dependencies": {
    "aws-cdk-lib": "2.161.1",
    "constructs": "^10.0.0"
  },
  "keywords": [
    "aws-cdk",
    "legal-tech",
    "mass-tort",
    "dynamodb",
    "sherlock-ai"
